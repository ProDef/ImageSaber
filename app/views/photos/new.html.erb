<h1>New Photo</h1>

<%= form_for Photo.new, :html => { :multipart => true }, url: photos_path do |f| %>
   <%= f.label :title %>
   <%= f.text_field :title %>
  
   <%= f.label :image %>
   <%= f.file_field :image %>

   <%= f.label :tag_names, 'Tags' %>
   <%= f.text_field :tag_names %>
 
   <%= f.submit 'add photo'%>
  <% end %>

  <%= link_to "home", photos_path %>